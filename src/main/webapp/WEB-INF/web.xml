<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>UserAccessManagementSystem</display-name>
  <servlet>
  <servlet-name>abc</servlet-name>
  <servlet-class>com.example.SignUpServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>abc</servlet-name>
  <url-pattern>/signup</url-pattern>
  </servlet-mapping>
   <servlet>
  <servlet-name>xyz</servlet-name>
  <servlet-class>com.example.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>xyz</servlet-name>
  <url-pattern>/login</url-pattern>
  </servlet-mapping>
   <servlet>
  <servlet-name>req</servlet-name>
  <servlet-class>com.example.RequestServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>req</servlet-name>
  <url-pattern>/request</url-pattern>
  </servlet-mapping>
  <servlet>
  <servlet-name>create</servlet-name>
  <servlet-class>com.example.SoftwareServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>create</servlet-name>
  <url-pattern>/create</url-pattern>
  </servlet-mapping>
  <servlet>
 <servlet-name>list</servlet-name>
 <servlet-class>com.example.SoftwareListServlet</servlet-class> 
 </servlet>
 <servlet-mapping>
 <servlet-name>list</servlet-name>
 <url-pattern>/softwareList</url-pattern>
 </servlet-mapping>
 <servlet>
 <servlet-name>pending</servlet-name>
 <servlet-class>com.example.PendingRequestsServlet</servlet-class> 
 </servlet>
 <servlet-mapping>
 <servlet-name>pending</servlet-name>
 <url-pattern>/pending</url-pattern>
 </servlet-mapping>
 
 <servlet>
 <servlet-name>approval</servlet-name>
 <servlet-class>com.example.ApprovalServlet</servlet-class> 
 </servlet>
 <servlet-mapping>
 <servlet-name>approval</servlet-name>
 <url-pattern>/approval</url-pattern>
 </servlet-mapping>
</web-app>